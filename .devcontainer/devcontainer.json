{
    "name": "TurtleBot3 ROS Humble Dev",
    "build": {
        "context": "..",
        "dockerfile": "dockerfile"
    },
    "runArgs": [
        "--net=host",
        "--env=DISPLAY",
        "--env=QT_X11_NO_MITSHM=1",
        "--volume=/tmp/.X11-unix:/tmp/.X11-unix:rw",
        "--volume=${localWorkspaceFolder}:/root/turtlebot3_ws/src:rw"
    ],
    "workspaceFolder": "/root/turtlebot3_ws",
    "remoteUser": "root"
}
